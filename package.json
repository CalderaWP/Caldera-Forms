		"grunt-contrib-watch": "^1.1.0",
		"grunt-exec": "^3.0.0",
		"grunt-mkdir": "^1.0.0",
		"grunt-newer": "^1.2.0",
		"grunt-remove": "^0.1.0",
		"grunt-text-replace": "^0.4.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.8.0",
		"jest-cli": "^24.8.0",
		"jest-fetch-mock": "^3.0.1",
