    "@wordpress/editor": "^9.3.0",
    "@wordpress/element": "^2.4.0",
    "@wordpress/i18n": "^3.3.0",
    "@wordpress/redux-routine": "^3.3.0",
    "@wordpress/url": "^2.3.0",
    "autoprefixer": "^8.6.5",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "graceful-fs": "^4.1.15",
    "grunt": "~0.4.2",
