		"@wordpress/url": "^3.27.1",
		"acorn": "^8.8.2",
		"autoprefixer": "^9.7.3",
		"axios": "^1.3.6",
		"axios-extensions": "^3.1.6",
		"babel-loader": "^8.0.5",
		"bootstrap": "^4.1.3",
		"classnames": "^2.2.6",
