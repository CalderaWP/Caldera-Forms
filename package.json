		"@wordpress/editor": "^9.3.0",
		"@wordpress/element": "^2.10.0",
		"@wordpress/i18n": "^3.3.0",
		"@wordpress/redux-routine": "^3.3.0",
		"@wordpress/url": "^2.3.0",
		"autoprefixer": "^9.7.3",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.5",
		"bootstrap": "^4.1.3",
		"classnames": "^2.2.6",
		"copy-webpack-plugin": "^5.1.1",
		"cross-env": "^6.0.3",
		"cypress": "^3.8.1",
		"enzyme": "^3.7.0",
