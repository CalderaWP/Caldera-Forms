		"@babel/polyfill": "^7.4.4",
		"@caldera-labs/api-client": "^0.6.1",
		"@caldera-labs/state": "^0.9.0",
		"@calderajs/babel-preset-calderajs": "^0.9.6",
		"@cypress/webpack-preprocessor": "^5.17.0",
		"@wordpress/babel-preset-default": "^7.10.2",
		"@wordpress/blocks": "^12.3.3",
		"@wordpress/browserslist-config": "^5.9.1",
		"@wordpress/components": "^23.3.6",
		"@wordpress/compose": "^6.3.3",
