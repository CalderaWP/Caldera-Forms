		"promise-polyfill": "^8.1.3",
		"raw-loader": "^4.0.0",
		"react": "16.9.0",
		"react-bootstrap": "^0.32.4",
		"react-dom": "^16.9.0",
		"react-redux": "^5.1.1",
		"react-test-renderer": "^16.9.0",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0",
		"rimraf": "^2.6.1",
