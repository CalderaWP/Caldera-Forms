	},
	"jest": {
		"collectCoverageFrom": [
			"clients/**/*.js"
		],
		"coverageThreshold": {
			"global": {
				"statements": 90,
				"branches": 90,
				"functions": 90,
				"lines": 90
			}
		},
		"automock": false,
		"setupFiles": [
			"./clients/tests/setupJest.js"
		],
		"testPathIgnorePatterns": [
			"cypress",
			"node_modules",
			"wordpress"
		]
	},
	"unpkg": "dist/index.js",
	"jsdelivr": "dist/index.js"
}
